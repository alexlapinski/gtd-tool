type Project {
  id: ID! @id
  name: String!
}

type Tag {
  id: ID! @id
  name: String!
}

type Task {
  id: ID! @id
  createdAt: DateTime! @createdAt
  modifiedAt: DateTime! @modifiedAt
  text: String!
  asignee: String!
  creator: String!
  priority: Int!
  dueDate: DateTime!
  tags: [Tag!]!
  project: Project!
}

type User {
  id: ID! @id
  email: String! @unique
  password: String!
  name: String!
}